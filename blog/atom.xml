---
layout: feed
---
<?xml version="1.0" encoding="utf-8" ?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Reverie Visuals Journal</title>
  <link href="https://reverievisuals.com/blog/atom.xml" rel="self"/>
  <link href="https://reverievisuals.com/blog"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://reverievisuals.com/blog</id>
  <author>
    <name>Tharawut Paripaiboon</name>
    <email>xcession@reverievisuals.com</email>
  </author>

  {% for post in site.posts %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="https://reverievisuals.com{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>https://reverievisuals.com{{ post.id }}</id>
      <content type="html">{{ post.content }}</content>
    </entry>
  {% endfor %}

</feed>